material {
	name : "Built-in fallback lit material",
	shadingModel : unlit,
	blending : opaque,
	requires: [ tangents ]
}

fragment {
    void material(inout MaterialInputs material) {
        prepareMaterial(material);

        material.baseColor = vec4((getWorldGeometricNormalVector() + 1.0) * 0.5, 1.0);
	}
}

